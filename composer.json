{
    "name": "marcusmyers/laravel-dashboard-accuweather-tile",
    "description": "An AccuWeather Tile for Laravel Dashboard",
    "keywords": [
        "marcusmyers",
        "laravel-dashboard-accuweather-tile"
    ],
    "homepage": "https://github.com/marcusmyers/laravel-dashboard-accuweather-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Myers",
            "email": "marcusmyers@gmail.com",
            "homepage": "https://emoxter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MarcusMyers\\AccuWeatherTile\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcusMyers\\AccuWeatherTile\\AccuWeatherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
